{
  "name": "toy-element",
  "version": "1.0.1",
  "description": "Components library by Vue3 + Ts",
  "type": "module",
  "main": "./dist/umd/index.umd.cjs",
  "module": "./dist/es/index.js",
  "types": "./dist/types/core/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/core/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/umd/index.umd.cjs"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "sideEffects": [
    "./dist/index.css",
    "./dist/theme/*.css"
  ],
  "scripts": {
    "build": "run-p build-es build-umd",
    "build:watch": "run-p build-es:watch build-umd:watch",
    "build-es": "vite build --config build/vite.esm.config.ts",
    "build-umd": "vite build --config build/vite.umd.config.ts",
    "build-es:watch": "vite build --watch --mode development --config build/vite.esm.config.ts",
    "build-umd:watch": "vite build --watch --mode development --config build/vite.umd.config.ts",
    "release": "release-it"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/vue-fontawesome": "^3.1.1",
    "@toy-element/components": "workspace:*"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "rollup-plugin-visualizer": "^6.0.3",
    "terser": "^5.43.1",
    "vite-plugin-compression2": "^2.2.0"
  },
  "peerDependencies": {
    "vue": "^3.5.18"
  }
}
